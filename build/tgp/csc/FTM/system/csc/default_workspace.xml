                launcher:packageName="com.gameloft.android.GloftANIF" 
                launcher:className="com.gameloft.android.GloftANIF.Start" />
            <!-- Dragon Mania -->
            <favorite
                launcher:screen="1" 
                launcher:packageName="com.gameloft.android.GloftPDMF" 
                launcher:className="com.gameloft.android.GloftPDMF.Start" />
            <!-- Puzzle Pets -->
            <favorite
                launcher:screen="2" 
                launcher:packageName="com.gameloft.android.GloftDBMF" 
                launcher:className="com.gameloft.android.GloftDBMF.Start" />
            <!-- Spider-Man: Ultimate Power -->
            <favorite
                launcher:screen="3" 
                launcher:packageName="com.gameloft.android.GloftSMIF" 
                launcher:className="com.gameloft.android.GloftSMIF.Start" />
        </folder> 


        <!-- Word -->
        <favorite
            launcher:packageName="com.microsoft.office.word" 
            launcher:className="com.microsoft.office.apphost.LaunchActivity" 
            launcher:screen="1" 
            launcher:x="2" 
            launcher:y="3" />

        <!-- Folder : Microsoft Apps -->
        <folder
            launcher:screen="1" 
            launcher:title="Microsoft Apps" 
            launcher:x="3" 
            launcher:y="3" >
            <!-- Excel -->
            <favorite
                launcher:screen="0" 
                launcher:packageName="com.microsoft.office.excel" 
                launcher:className="com.microsoft.office.apphost.LaunchActivity" />
            <!-- PowerPoint -->
            <favorite
                launcher:screen="1" 
                launcher:packageName="com.microsoft.office.powerpoint" 
                launcher:className="com.microsoft.office.apphost.LaunchActivity" />
            <!-- OneDrive -->
            <favorite
                launcher:screen="2" 
                launcher:packageName="com.microsoft.skydrive" 
                launcher:className="com.microsoft.skydrive.MainActivity" />
            <!-- OneNote -->
            <favorite
                launcher:screen="3" 
                launcher:packageName="com.microsoft.office.onenote" 
                launcher:className="com.microsoft.office.onenote.ui.ONMSplashActivity" />
        </folder> 


    </home> 

