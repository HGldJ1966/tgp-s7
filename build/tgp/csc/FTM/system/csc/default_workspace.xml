<favorites xmlns:launcher="http://schemas.android.com/apk/res/com.sec.android.app.launcher">
    <home>
        <!--  screen [0]  -->
        <!-- .appwidget.WeatherAppWidget2x1 -->
        <appwidget
            launcher:packageName="com.sec.android.daemonapp" 
            launcher:className="com.sec.android.daemonapp.appwidget.WeatherAppWidget2x1" 
            launcher:screen="0" 
            launcher:x="0" 
            launcher:y="0" 
            launcher:spanX="2" 
            launcher:spanY="1" />


        <!-- Orange et moi -->
        <favorite
            launcher:packageName="com.orange.orangeetmoi" 
            launcher:className="com.orange.orangeetmoi.OrangeEtMoi" 
            launcher:screen="0" 
            launcher:x="0" 
            launcher:y="1" />

        <!-- Folder : Orange -->
