<favorites xmlns:launcher="http://schemas.android.com/apk/res/com.sec.android.app.launcher">
    <home>
        <!--  screen [0]  -->
        <!-- Camera -->
        <favorite
            launcher:packageName="com.sec.android.app.camera" 
            launcher:className="com.sec.android.app.camera.Camera" 
            launcher:screen="0" 
            launcher:x="0" 
            launcher:y="3" />

        <!-- Gallery -->
        <favorite
            launcher:packageName="com.sec.android.gallery3d" 
            launcher:className="com.sec.android.gallery3d.app.GalleryOpaqueActivity" 
            launcher:screen="0" 
            launcher:x="1" 
            launcher:y="3" />

        <!-- Galaxy Apps -->
        <favorite
            launcher:packageName="com.sec.android.app.samsungapps" 
            launcher:className="com.sec.android.app.samsungapps.SamsungAppsMainActivity" 
            launcher:screen="0" 
            launcher:x="3" 
            launcher:y="3" />

        <!--  screen [1]  -->
        <!-- .SearchWidgetProvider -->
        <appwidget
            launcher:packageName="com.google.android.googlequicksearchbox" 
            launcher:className="com.google.android.googlequicksearchbox.SearchWidgetProvider" 
            launcher:screen="1" 
            launcher:x="0" 
            launcher:y="0" 
            launcher:spanX="4" 
            launcher:spanY="1" />


        <!-- Folder : Three -->
        <folder
            launcher:screen="1" 
            launcher:title="Three" 
            launcher:x="3" 
            launcher:y="2" >
            <!-- Three -->
            <favorite
                launcher:screen="0" 
                launcher:packageName="com.hutchison3g.planet3" 
                launcher:className="com.hutchison3g.planet3.Planet3Activity" />
            <!-- Three inTouch -->
            <favorite
                launcher:screen="1" 
                launcher:packageName="com.hutchison3g.threeintouch" 
                launcher:className="com.mavenir.android.vtow.activity.SplashScreenActivity" />
            <!-- Three Likes -->
            <favorite
                launcher:screen="2" 
                launcher:packageName="com.hutchison3g.threelikes" 
                launcher:className="com.hutchison3g.threelikes.ThreeLikesActivity" />
        </folder> 


        <!-- Email -->

        <favorite
            launcher:packageName="com.samsung.android.email.provider" 
            launcher:className="com.samsung.android.email.ui.activity.MessageListXL" 
            launcher:screen="1" 
            launcher:x="0" 
            launcher:y="3" />

        <!-- Samsung Pay -->
	<!-- 
        <favorite
            launcher:packageName="com.samsung.android.spay" 
            launcher:className="com.samsung.android.spay.ui.SpayMainActivity" 
            launcher:screen="1" 
            launcher:x="0" 
            launcher:y="3" />
        -->

        <!-- YouTube -->
        <favorite
            launcher:packageName="com.google.android.youtube" 
            launcher:className="com.google.android.youtube.app.honeycomb.Shell$HomeActivity" 
            launcher:screen="1" 
            launcher:x="1" 
            launcher:y="3" />

        <!-- Maps -->
        <favorite
            launcher:packageName="com.google.android.apps.maps" 
            launcher:className="com.google.android.maps.MapsActivity" 
            launcher:screen="1" 
            launcher:x="2" 
            launcher:y="3" />

        <!-- Play Store -->
        <favorite
            launcher:packageName="com.android.vending" 
            launcher:className="com.android.vending.AssetBrowserActivity" 
            launcher:screen="1" 
            launcher:x="3" 
            launcher:y="3" />

        <!--  screen [2]  -->
        <!-- .WidgetProvider42 -->
        <appwidget
            launcher:packageName="com.sec.android.widgetapp.samsungapps" 
            launcher:className="com.sec.android.widgetapp.samsungapps.WidgetProvider42" 
            launcher:screen="2" 
            launcher:x="0" 
            launcher:y="0" 
            launcher:spanX="4" 
            launcher:spanY="1" />


        <!-- com.deezer.android.appwidget.DeezerWidgetProvider -->
        <appwidget
            launcher:packageName="deezer.android.app" 
            launcher:className="com.deezer.android.appwidget.DeezerWidgetProvider" 
            launcher:screen="2" 
            launcher:x="0" 
            launcher:y="1" 
            launcher:spanX="4" 
            launcher:spanY="1" />


        <!-- Amazon -->
        <favorite
            launcher:packageName="uk.amazon.mShop.android" 
            launcher:className="com.amazon.mShop.home.HomeActivity" 
            launcher:screen="2" 
            launcher:x="0" 
            launcher:y="2" />

        <!-- Folder : Social -->
        <folder
            launcher:screen="2" 
            launcher:title="Social" 
            launcher:x="2" 
            launcher:y="2" >
            <!-- Instagram -->
            <favorite
                launcher:screen="0" 
                launcher:packageName="com.instagram.android" 
                launcher:className="com.instagram.android.activity.MainTabActivity" />
            <!-- WhatsApp -->
            <favorite
                launcher:screen="1" 
                launcher:packageName="com.whatsapp" 
                launcher:className="com.whatsapp.Main" />
        </folder> 


        <!-- Facebook -->
        <favorite
            launcher:packageName="com.facebook.katana" 
            launcher:className="com.facebook.katana.LoginActivity" 
            launcher:screen="2" 
            launcher:x="3" 
            launcher:y="2" />

        <!-- eBay -->
        <favorite
            launcher:packageName="com.ebay.mobile" 
            launcher:className="com.ebay.mobile.activities.eBay" 
            launcher:screen="2" 
            launcher:x="0" 
            launcher:y="3" />

        <!-- Yelp -->
        <favorite
            launcher:packageName="com.yelp.android" 
            launcher:className="com.yelp.android.ui.activities.RootActivity" 
            launcher:screen="2" 
            launcher:x="1" 
            launcher:y="3" />

        <!-- Word -->
        <favorite
            launcher:packageName="com.microsoft.office.word" 
            launcher:className="com.microsoft.office.apphost.LaunchActivity" 
            launcher:screen="2" 
            launcher:x="2" 
            launcher:y="3" />

        <!-- Folder : Microsoft Apps -->
        <folder
            launcher:screen="2" 
            launcher:title="Microsoft Apps" 
            launcher:x="3" 
            launcher:y="3" >
            <!-- Excel -->
            <favorite
                launcher:screen="0" 
                launcher:packageName="com.microsoft.office.excel" 
                launcher:className="com.microsoft.office.apphost.LaunchActivity" />
            <!-- PowerPoint -->
            <favorite
                launcher:screen="1" 
                launcher:packageName="com.microsoft.office.powerpoint" 
                launcher:className="com.microsoft.office.apphost.LaunchActivity" />
            <!-- OneNote -->
            <favorite
                launcher:screen="2" 
                launcher:packageName="com.microsoft.office.onenote" 
                launcher:className="com.microsoft.office.onenote.ui.ONMSplashActivity" />
            <!-- OneDrive -->
            <favorite
                launcher:screen="3" 
                launcher:packageName="com.microsoft.skydrive" 
                launcher:className="com.microsoft.skydrive.MainActivity" />
            <!-- Skype -->
            <favorite
                launcher:screen="4" 
                launcher:packageName="com.skype.raider" 
                launcher:className="com.skype.raider.Main" />
        </folder> 


    </home> 

    <!-- Hotseat (We use the screen as the position of the item in the hotseat) -->
    <hotseat>
        <!-- Phone -->
        <favorite
            launcher:packageName="com.android.contacts" 
            launcher:className="com.android.dialer.DialtactsActivity" 
            launcher:screen="0" />

        <!-- Messages -->
        <favorite
            launcher:packageName="com.android.mms" 
            launcher:className="com.android.mms.ui.ConversationComposer" 
            launcher:screen="1" />

        <!-- Folder : Google -->
        <folder
            launcher:screen="2" 
            launcher:title="Google" 
            launcher:x="3" 
            launcher:y="0" >
            <!-- Google -->
            <favorite
                launcher:screen="0" 
                launcher:packageName="com.google.android.googlequicksearchbox" 
                launcher:className="com.google.android.googlequicksearchbox.SearchActivity" />
            <!-- Chrome -->
            <favorite
                launcher:screen="1" 
                launcher:packageName="com.android.chrome" 
                launcher:className="com.google.android.apps.chrome.Main" />
            <!-- Gmail -->
            <favorite
                launcher:screen="2" 
                launcher:packageName="com.google.android.gm" 
                launcher:className="com.google.android.gm.ConversationListActivityGmail" />
            <!-- Drive -->
            <favorite
                launcher:screen="3" 
                launcher:packageName="com.google.android.apps.docs" 
                launcher:className="com.google.android.apps.docs.app.NewMainProxyActivity" />
            <!-- Play Music -->
            <favorite
                launcher:screen="4" 
                launcher:packageName="com.google.android.music" 
                launcher:className="com.android.music.activitymanagement.TopLevelActivity" />
            <!-- Play Movies & TV -->
            <favorite
                launcher:screen="5" 
                launcher:packageName="com.google.android.videos" 
                launcher:className="com.google.android.youtube.videos.EntryPoint" />
            <!-- Hangouts -->
            <favorite
                launcher:screen="6" 
                launcher:packageName="com.google.android.talk" 
                launcher:className="com.google.android.talk.SigningInActivity" />
            <!-- Photos -->
            <favorite
                launcher:screen="7" 
                launcher:packageName="com.google.android.apps.photos" 
                launcher:className="com.google.android.apps.photos.home.HomeActivity" />
        </folder> 


    </hotseat> 

</favorites> 